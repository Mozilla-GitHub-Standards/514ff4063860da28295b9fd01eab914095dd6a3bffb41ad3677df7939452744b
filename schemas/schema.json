{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "shield-cloud-storage",
  "description": "cloud storage telemetry data",
  "type": "object",
  "definitions": {
    "branch_message": {
      "type": "string",
      "enum": ["Addon_init", "prompt_save_click", "prompt_opted_in", "prompt_cancel_click", "prompt_rejected", "download_started"]
    },
    "provider_key": {
      "type": "string",
      "enum": ["Dropbox", "GDrive"]
    },
    "notification_instance": {
      "message": { "$ref": "#/definitions/branch_message" },
      "provider": { "$ref": "#/definitions/provider_key" },
      "timestamp": { "type": "string" },
      "provider_count": { "type": "string" },
    }
  }
}
